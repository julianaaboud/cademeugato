        </activity>
        <activity android:name=".Activity.MenuActivity" />
        <activity android:name=".Activity.CadastroUsuarioActivity" />
        <activity android:name=".Activity.PerfilActivity" />
        <activity android:name=".Activity.CadastroGatoActivity"></activity>
